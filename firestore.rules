rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure and are for development purposes only.
    // They allow any authenticated user to read and write any data.
    // TODO: Implement more granular security rules before production deployment.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
