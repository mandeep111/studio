
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are completely open and insecure. They are
    // intended only for initial development to prevent permission errors.
    // You MUST secure your database with proper rules before deploying
    // to production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
